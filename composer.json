{
    "name": "hamaka/silverstripe-subsite-switch-own-domain",
    "description": "Open CMS interface for subsite on own domain",
    "type": "silverstripe-vendormodule",
    "homepage": "https://github.com/hamaka/silverstripe-subsite-switch-own-domain",
    "keywords": [
        "silverstripe",
        "CMS",
        "subsite"
    ],
    "license": "BSD-3-Clause",
    "require": {
        "php": ">=7.1",
        "silverstripe/cms": "^4",
        "silverstripe/subsites": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "hamaka\\silverstripe-subsite-switch-own-domain\\": "src/"
        }
    },
    "support": {
        "issues": "https://github.com/hamaka/silverstripe-subsite-switch-own-domain/issues"
    },
    "extra": {
        "installer-name": "Silverstripe Subsite on own domain",
        "branch-alias": {
            "dev-master": "2.x-dev"
        },
        "expose": [
            "client/dist"
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
